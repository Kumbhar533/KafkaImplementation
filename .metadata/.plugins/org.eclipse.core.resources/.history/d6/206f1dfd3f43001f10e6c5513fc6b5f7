package com.nilesh.kafka.consumer.service;

import org.springframework.stereotype.Service;

import com.nilesh.kafka.consumer.orderEntity;

@Service
public class consumerService {
	
	
	
	public void getAllOrders(orderEntity entity) {
		
		System.out.println(entity.getOrderId()+"\n"+entity.getOrderPrice()+"\n"+entity.getLocation()+"\n"+entity.getDeliveryDate()+"\n"+
		                       entity.getShippingDate());
	}
	
	

}
